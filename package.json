{
  "name": "ng-flare",
  "version": "0.1.3",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run config",
    "start": "ng serve",
    "prebuild": "npm run config",
    "build": "ng build",
    "pretest": "npm run config",
    "test": "ng test",
    "prelint": "npm run config",
    "lint": "ng lint",
    "pree2e": "npm run config && npm run config:e2e",
    "e2e": "ng e2e",
    "smoke": "npx ts-node tools/smoke-server",
    "config:e2e": "npx ts-node --project=tools/tsconfig.tools.json tools/set-e2e-env.ts",
    "config": "npx ts-node --project=tools/tsconfig.tools.json tools/set-env.ts",
    "indexes:get": "npx firebase firestore:indexes > firestore.indexes.json",
    "indexes:deploy": "npx firebase deploy --only firestore:indexes",
    "rules:deploy": "npx firebase deploy --only firestore:rules",
    "config:clean": "npx ts-node --project=tools/tsconfig.tools.json tools/clear-env.ts",
    "clean": "npx rimraf dist out-tsc coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "~8.0.3",
    "@angular/compiler": "~8.0.3",
    "@angular/core": "~8.0.3",
    "@angular/fire": "^5.1.3",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.0.3",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "~8.0.3",
    "@angular/platform-browser-dynamic": "~8.0.3",
    "@angular/router": "~8.0.3",
    "@ngrx/effects": "^8.0.1",
    "@ngrx/entity": "^8.0.1",
    "@ngrx/store": "^8.0.1",
    "@ngrx/store-devtools": "^8.0.1",
    "core-js": "^3.1.4",
    "firebase": "^6.2.4",
    "luxon": "^1.13.2",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/cli": "~8.0.6",
    "@angular/compiler-cli": "~8.0.3",
    "@angular/language-service": "~8.0.3",
    "@nrwl/builders": "7.8.7",
    "@nrwl/cypress": "^8.2.0",
    "@nrwl/workspace": "^8.2.0",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.3",
    "@types/luxon": "^1.13.0",
    "@types/node": "~12.0.10",
    "codelyzer": "^5.0.1",
    "cypress": "^3.3.2",
    "dotenv": "^8.0.0",
    "firebase-tools": "^7.2.1",
    "husky": "^2.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "live-server": "^1.2.1",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "ts-node": "^8.3.0",
    "tslint": "~5.18.0",
    "typescript": "^3.4.3",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
